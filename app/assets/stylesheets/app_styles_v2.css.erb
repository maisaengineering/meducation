html {
    height: 100%;
    margin: 0px;
    padding: 0px;
}

body {
    /*font-family: 'ConeriaScriptMedium';*/
    font-family: "museo-sans-n1", "museo-sans-i1", "museo-sans-n3", "museo-sans-i3", "museo-sans-n5", "museo-sans-i5", "museo-sans-n7", "museo-sans-i7", "museo-sans-n9", "museo-sans", "Segoe UI", "Trebuchet MS", Arial, Helvetica, sans-serif;
    font-size: 13px;
    font-weight: 300;
    color: #555;
    background-color: #fff;
    background-image: url(<%= asset_path('h_bg.png')%>);
    background-repeat: repeat-x;
    position: relative;
    margin: 0px;
    padding: 0px;
    padding-left: 5px;
    padding-right: 10px;
    height: 100%;
}

img {
    border: none;
}
a {
    color: inherit;
}
a:hover {
    color: #000;
}

h1 {
    font-weight: 700;
    font-size: 18px;
}

form {
    padding: 0px;
    margin: 0px;
}

.clear {
    clear: both;
}

b {
    font-weight: 700;
    font-size: inherit;
}

b.bBlue {
    color: #13a3de !important;
}

b.bOrange {
    color: #fab138 !important;
}
strong.bBlue {
    color: #13a3de !important;
}
strong.bOrange {
    color: #fab138 !important;
}



i {
    font-style: italic;
    font-size: inherit;
}
    /* start page */

body.startBody {
    padding-left: 0px;
    padding-right: 10px;
    background-image: none;
}

.startOuterTable {
    height: 100%;
    width: 100%;
}

.startOuterTable td.sotTop {
    background-color: #c4eafa;
    background-image: url(<%= asset_path('l_start_bg_kids.png')%>);
    background-repeat: repeat;
    background-position: bottom left;
    border-bottom: solid 5px #c4eafa;
}

.startOuterTable td.sotMiddle {
    height: 577px;
    background-color: #fff;
    overflow: visible;
    padding-top: 5px;
}

.startOuterTable td.sotBottom {
    border-top: solid 5px #c4eafa;
    background-color: #c4eafa;
    background-image: url(<%= asset_path('l_start_bg_kids.png')%>);
    background-repeat: repeat;
    background-position: left -13px;
}

.startContainer {
    margin-left: auto;
    margin-right: auto;
    width: 1000px;
    height: 577px;
}

.startPhotoContainer {
    width: 343px;
    height: 577px;
    margin-left: 20px;
    float: left;
    background-image: url(<%= asset_path('l_bigphoto_1.jpg')%>);
    background-repeat: no-repeat;
    background-color: #f000;
    position: relative;
    left: 15px;
}

.startLinksContainer {
    margin-top: 482px;
    font-size: 13px;
    color: #aaa;
    font-weight: 300;
}

.startLinksContainer ul {
    padding: 0px;
    margin: 0px;
}

.startLinksContainer li {
    padding: 0px;
    margin: 0px;
    list-style: none;
    margin-bottom: 3px;
}

.startLinksContainer a {
    text-decoration: none;
}

.startRightContainer {
    width: 637px;
    float: left;
}

h1.startTopHeader {
    height: 76px;
    margin-top: 97px;
    font-weight: 100;
    font-size: 33px;
    color: #7a7a7a;
    text-align: center;
    line-height: 34px;
    text-transform: lowercase;
}

h1.startTopHeader .startTopHeaderBold {
    font-weight: 700;
}

h1.startTopHeader .startTopSubheader {
    display: block;
    font-size: 30px;
}

.startBigButtonContainer {
    width: 415px;
    margin-left: auto;
    margin-right: auto;
    height: 81px;
    margin-top: 13px;
}

.startBigButton {
    border-radius: 10px;
    box-shadow: 4px 4px 7px #777;
    color: #13a3de;
    font-size: 24px;
    padding-left: 30px;
    padding-right: 30px;
    padding-top: 8px;
    padding-bottom: 12px;
    font-weight: 500;
    background-color: #fff;
    text-transform: lowercase;
    cursor: pointer;
}

.sbbSignUp {
    background-image: url(<%= asset_path('d_button_bg_medblue.png')%>);
    background-repeat: repeat-x;
    float: left;
}

.sbbLearnMore {
    float: right;
}

.startBigButton a {
    text-decoration: none;
}

.startBigButton:hover a {
    color: #000;
}

.startExplanationContainer {
    background-image: url(<%= asset_path('l_startLogo.png')%>);
    background-repeat: no-repeat;
    background-position: 3px 23px;
    height: 74px;
    width: 295px;
    margin-left: auto;
    margin-right: auto;
    padding-top: 23px;
    padding-bottom: 48px;
    padding-left: 122px;
    text-align: center;
    font-weight: 300;
    color: #a1a1a1;
    font-size: 14px;
    line-height: 1.3em;
}

.startExplanationContainer b {
    color: #777;
}

.startLoginContainer .startSealContainer {
    position: absolute;
    left: -117px;
    top: 90px;
    width: 105px;
    height: 55px;
}

.startLoginContainer {
    border-radius: 10px;
    box-shadow: 4px 4px 7px #777;
    background-color: #d7f1fb;
    background-image: url(<%= asset_path('l_panel_bg_medblue.png')%>);
    background-repeat: repeat-x;
    background-position: top;
    height: 163px;
    padding-top: 3px;
    width: 415px;
    margin-left: auto;
    margin-right: auto;
    margin-top: -10px;
    position: relative;
    top: 10px;
}

.startLoginLeft {
    width: 195px;
    padding-left: 20px;
    float: left;
}

.startLoginHeader {
    color: #13a3de;
    font-weight: 300;
    font-size: 17px;
    padding-top: 10px;
    padding-bottom: 5px;
    text-transform: lowercase;
}

.startLoginInput {
    width: 100%;
    height: 29px;
    border: solid 1px #ccc;
    font-size: 14px;
    border-radius: 7px;
    margin: 0px;
    padding: 0px;
    padding-left: 10px;
    padding-right: 10px;
    margin-top: 10px;
    margin-bottom: 5px;
    color: #555;
}

.startLoginForgotContainer {
    font-size: 11px;
    text-align: center;
}

.startLoginRight {
    width: 133px;
    padding-left: 33px;
    float: left;
}

.startSignInButton {
    border-radius: 7px;
    box-shadow: 4px 4px 7px #777;
    color: #fff;
    font-size: 16px;
    width: 100%;
    padding-top: 5px;
    padding-bottom: 6px;
    font-weight: 700;
    background-color: #28ace2;
    text-transform: lowercase;
    cursor: pointer;
    background-image: url(<%= asset_path('d_button_bg_dkblue.png')%>);
    background-repeat: repeat-x;
    text-align: center;
    margin-top: 45px;
}

.startSignInButton a {
    text-decoration: none;
}

.startSignInButton:hover a {
    color: #000;
}

.startLoginRememberContainer {
    font-size: 13px;
    padding-top: 15px;
}

.startLoginRememberContainer input[type=checkbox] {
    float: right;
    margin-left: 5px;
    margin-right: 10px;
    margin-top: 10px;
}

    /* page structure */

.mainContainer {
    margin: 5px;
    padding: 5px;
    margin-right: auto;
    margin-left: auto;
    width: 975px;
}

.header {
    padding-bottom: 10px;
    position: relative;
}

.header .userBox {
    font-size: 10px;
    float: right;
    margin-top: 80px;
    text-align: right;
}

.header .headerBox {
    border-radius: 10px;
    box-shadow: 4px 4px 7px #777;
    background-image: url(<%= asset_path('h_headerBox_bg.png')%>);
    background-repeat: repeat-y;
    height: 115px;
    float: left;
    vertical-align: auto;
    padding-right: 10px;
    padding-left: 10px;
    position: relative;
    z-index: 0;
}

.header .headerBox a {
    text-decoration: none;
}

.header .logoBox {
    width: 122px;
    vertical-align: auto;
    float: left;
}

.header #klLogo {
    width: 102px;
    height: 75px;
    background-image: url(<%= asset_path('h_logo.png')%>);
    margin-top: 20px;
    margin-left: 10px;
    background-repeat: no-repeat;
    display: block;
}

.header .sponsorBox {
    width: 190px;
    height: 110px;
    background-repeat: no-repeat;
    background-position: center center;
    float: left;
}

.header .myKids {
    color: #fab138;
    font-weight: 700;
    font-size: 15px;
    margin-top: 78px;
    float: left;
    text-transform: lowercase;
    margin-left: 15px;
    margin-right: 20px;
}

.header .childPhotoBox {
    width: 70px;
    height: 20px;
    margin-left: 10px;
    margin-right: 5px;
    padding-left: 0px;
    padding-top: 90px;
    /*background-image: url(<%= asset_path('d_defaultavatar.png')%>);*/
    background-repeat: no-repeat;
    background-position: 0px 8px;
    text-align: center;
    font-size: 12px;
    font-weight: 700;
    color: #fff;
    text-shadow: 0.1em 0.1em .5em #000;
    float: left;
    cursor: pointer;
}

.header .childSummaryBox {
    width: 215px;
    height: 115px;
    float: left;
}
.header .childSummaryBox.selected {
    height: 145px;
    background-color: #fff;
    background-image: url(<%= asset_path('h_childSelectedArrow_bg.png')%>);
    background-repeat: no-repeat;*
position: relative;
    z-index: 1;
    font-size: 11px;
    color: #666;
    font-weight: 300;
    padding-top: 8px;
    padding-left: 8px;
    padding-right: 8px;
    margin-left: 2px;
}
.header .childSummaryBoxShadow {
    height: 115px;
    width: 234px;
    position: relative;
    background-color: #fff;
    z-index: -1;
    margin-left: -234px;
    box-shadow: 4px 2px 7px #777;
    float: left;
    margin-right: 10px;
}


.header .csbPhoto {
    float: left;
    padding-right: 5px;
}

.header .csbName {
    color: #000;
    font-weight: 700;
    display: block;
    font-size: 13px;
}

.header .csbID {
    display: block;
}

.header .csbAlerts {
    display: block;
    font-weight: 700;
    color: #9ba112;
    margin-top: 8px;
    margin-bottom: 7px;
}

.header .csbAlertsOn {
    color: #ed1c24;
}

.header .csbOrgList {
    list-style: none;
    margin: 0px;
    padding: 0px;
}

.header .csbOrgList li {
    list-style: none;
    margin-left: 0px;
}

.header .orgLogoBox {
    float: left;
    height: 100%;
}

.header .orgLogoBox table {
    height: 100%;
    margin: 0px;
    padding: 0px;
}

.header .orgLogoBox td {
    vertical-align: middle;
}

.header .navContainer {
    float: left;
    padding-top: 70px;
    padding-left: 15px;
}

.header .navTab {
    display: inline-block;
    padding: 5px;
    padding-left: 7px;
    padding-right: 7px;
    font-weight: 700;
    font-size: 13px;
    margin-left: 10px;
    color: #13a3de;
    text-transform: lowercase;
}

.header .navTab:hover {
    border-radius: 0px 0px 5px 5px;
    box-shadow: 4px 4px 7px #777;
}

.header .navTabOn {
    border-radius: 0px 0px 5px 5px;
    box-shadow: 4px 4px 7px #777;
}

.header .navTab a {
    text-decoration: none;
}

.header #tipClickToView {
    position: absolute;
    top: 107px;
    left: 350px;
}


.bodyContainer {
    clear: both;
    padding-top: 15px;
}


