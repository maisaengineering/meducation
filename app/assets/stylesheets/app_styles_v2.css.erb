    /* blue app footer -- only for tablets */

.appFooter {
    position: fixed;
    z-index: 1000;
    background-color: #2d677f;
    background: -webkit-linear-gradient(top, #558396, #2d677f 35%);
    background: -linear-gradient(top, #558396, #2d677f 35%);
    left: 0px;
    bottom: 0px;
    height: 50px;
    width: 100%;
    font-weight: 500;
    color: #c0e1ef;
}

.appFooterInside {
    width: 975px;
    margin-left: auto;
    margin-right: auto;
    height: 50px;
    border-left: solid 1px #c0e1ef;
}

.appFooter .grouping {
    float: left;
    margin-left: 0px;
    margin-right: 0px;
    height: 50px;
    border-right: solid 1px #c0e1ef;
    padding-right: 7px;
    padding-left: 2px;
}

.appFooter .groupingSelected {
    background: -webkit-linear-gradient(top, #4c8197 70%, #6a9aad);
    background: -linear-gradient(top, #4c8197 70%, #6a9aad);
}

.appFooter .groupingName {
    display: inline-block;
    height: 50px;
    font-weight: 300;
    font-size: 20px;
    color: #fff;
    padding-left: 10px;
    padding-right: 5px;
    text-align: center;
    cursor: pointer;
}

.appFooter .groupingName .inner {
    display: table-cell;
    vertical-align: middle;
    height: 50px;
}

.appFooter .groupingNameSelected {
    color: #fff;
}

.appFooter .groupingItem {
    display: inline-block;
    height: 50px;
    margin-left: 7px;
    margin-right: 7px;
    line-height: 1em;
    font-weight: 700;
    text-align: center;
    cursor: pointer;
    padding-left: 5px;
    padding-right: 5px;
    color: #e2f7ff;
}

.appFooter .groupingItem .inner {
    display: table-cell;
    vertical-align: middle;
    height: 50px;
}

.appFooter .groupingItemSelected {
    color: #fff;
    border-radius: 5px;
    background-color: #558396;
    background: -webkit-linear-gradient(top, #7fafc2, #6092a6 30%);
    background: -linear-gradient(top, #7fafc2, #6092a6 30%);
    height: 42px;
    margin-bottom: 4px;
}

.appFooter .groupingItemSelected .inner {
    height: 42px;
}



