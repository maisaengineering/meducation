    /* basic */

.bodyText {
    font-size: 14px;
    color: #666;
    font-weight: 300;
    width: 600px;
    line-height: 1.5em;
}

.bigLink {
    font-size: 16px;
    font-weight: 500;
}

.blueLink {
    color: #10a2dd;
}


    /* popups */

.popUpContainer {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
    margin: 0px;
    margin-top: -5px;
    z-index: 500;
}

.popUpBG {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
    margin: 0px;
    /*background-color: #09C;*/
    background-color: #2d677f;
    opacity: .6;
}

.popUpContainer .popUpTable {
    width: 100%;
    height: 100%;
    border: none;
    z-index: 501;
}

.popUpContainer .popUpTableTD {
    vertical-align: middle;
    align: center;
}

.popUpContent {
    width: 400px;
    margin: auto;
}

.popUpPanel {
    border-radius: 10px;
    box-shadow: 4px 4px 7px #777;
    background-color: #fff;
    /* overflow: hidden; */
    margin-bottom: 35px;
    padding: 15px;
    position: relative;
}

.popUpPanel h2 {
    color: #000;
    font-size: 15px;
    font-weight: 700;
    margin-top: 5px;
    margin-bottom: 10px;
}

.popUpPanel td {
    padding-right: 5px;
}

.popUpPanel input[type=submit] {
    background-color: #fab138;
    border-radius: 8px;
    box-shadow: 2px 2px 4px #777;
    font-size: 1.3em;
    color: #fff;
    font-weight: 500;
    border: none;
    padding: 5px;
    padding-left: 12px;
    padding-right: 12px;
    margin-top: 15px;
    text-transform: lowercase;
    cursor: pointer;
}

.popUpPanel input[type=submit].btnCancel {
    background-color: #ddd;
    color: #666;
    margin-left: 5px;
}

.popUpPanel input[type=text] {
    margin-top: 3px;
    margin-bottom: 3px;
    width: 200px;
    height: 23px;
    border: solid 1px #ccc;
    font-size: 14px;
}

.popUpPanel textarea {
    margin-top: 3px;
