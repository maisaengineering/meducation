/* popups */

.popUpContainer {
	/* Set rules to fill background */
	min-height: 100%;
	min-width: 1024px;
	/* Set up positioning */
	position: fixed;	
	left: 0;
	top: 0;
	bottom: 0;
	right: 0;

	margin: 0px;
	margin-top: -5px;
	z-index: 1000;
	/* Set up proportionate scaling */
	width: 100%;
	height: auto;		
}

.popUpBG {	
	min-height: 100%;
	min-width: 1024px;
	position: absolute;
	left: 0;
	top: 0;
	bottom: 0;
	right: 0;
	margin: 0px;
	background-color: #09C;
	opacity: .6;
}

.popUpContainer .popUpTable {
	width: 100%;
	height: 100%;
	border: none;
	z-index: 1001;
}

.popUpContainer .popUpTableTD {
	vertical-align: middle;
	align: center;
}

.popUpContent {
	width: 400px;
	margin: auto;
}

.popUpPanel {
	border-radius: 10px;
	box-shadow: 4px 4px 7px #777;
	background-color: #fff;
	/* overflow: hidden; */
	margin-bottom: 35px;
	padding: 15px;
	position: relative;
}

.popUpPanel h2 {
	color: #000;
	font-size: 15px;
	font-weight: 700;
	margin-top: 5px;
	margin-bottom: 10px;
}

.popUpPanel td {
	padding-right: 5px;
}

