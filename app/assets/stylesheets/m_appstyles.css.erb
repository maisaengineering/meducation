    /* START Back Button Hack */

.headerButtonContainer .hbBack {
    position: relative;
    overflow: visible;
    margin-left: 20px;
    border-radius: 0px 16px 16px 0px;
    background-color: #fff;
}

.headerButtonContainer .hbBackOne {
    position: absolute;
    display: inline-block;
    left: -21.5px;
    top: 8.8px;
    width: 42.4px;
    height: 42.4px;
    background: #fff;
    -webkit-transform: rotate(45deg);
    -moz-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    transform: rotate(45deg);
    box-shadow: 3px 3px 5px #aaa;
    z-index: 4;
}

.headerButtonContainer .hbBack:active .hbBackOne {
    background: none;
    background-color: #13a3de;
    color: #fff;
}

.headerButtonContainer .hbBackTwo {
    position: absolute;
    z-index: 5;
    width: 40px;
    height: 60px;
    background-color: #fff;
    left: -1px;
    border-radius: 4px;
}

.headerButtonContainer .hbBack:active .hbBackTwo {
    background: none;
    background-color: #13a3de;
    color: #fff;
}

.headerButtonContainer .hbBackThree {
    position: absolute;
    display: inline-block;
    left: -40px;
    top: -15px;
    z-index: 3;
    background-color: #fff;
    width: 40px;
    height: 90px;
}

.headerButtonContainer .hbBack span {
    position: relative;
    z-index: 6;
    left: -5px;
}

    /* END Back Button Hack */

