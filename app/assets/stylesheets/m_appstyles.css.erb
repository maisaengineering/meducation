html {
    height: 100%;
    margin: 0px;
    padding: 0px;
}

body {
    font-family: "museo-sans-n1", "museo-sans-i1", "museo-sans-n3", "museo-sans-i3", "museo-sans-n5", "museo-sans-i5", "museo-sans-n7", "museo-sans-i7", "museo-sans-n9", "museo-sans", "Segoe UI", "Trebuchet MS", Arial, Helvetica, sans-serif;
    font-size: 20px;
    font-weight: 300;
    color: #555;
    background-color: #fff;
    position: relative;
    margin: 0px;
    padding: 0px;
    padding-left: 5px;
    padding-right: 10px;
    height: 100%;
    -webkit-user-select: none;
    -webkit-touch-callout: none;
    -webkit-text-size-adjust: none;
}

img {
    border: none;
}
a {
    color: inherit;
}

h1 {
    font-weight: 700;
    font-size: 40px;
}

form {
    padding: 0px;
    margin: 0px;
}

input, select, textarea {
    font-family: "museo-sans", "Segoe UI", "Trebuchet MS", Arial, Helvetica, sans-serif;
    box-sizing: border-box;
}

.clear {
    clear: both;
}

b {
    font-weight: 700;
    font-size: inherit;
}

b.bBlue {
    color: #13a3de !important;
}

b.bOrange {
