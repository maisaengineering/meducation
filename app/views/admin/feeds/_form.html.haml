= form_for [:admin,@feed], authenticity_token: true do |f|
  -if @feed.errors.any?
    #error_explanation
      %h2= "#{pluralize(@feed.errors.count, "error")} prohibited this user from being saved:"
      %ul
        - @feed.errors.full_messages.each do |msg|
          %li= msg
  #dashEditForm.manageContainer
    %p.manageExplanation Enter or edit the form below.
    #dashEditFormPanel.dashPanel.managePanel
      %span.panelName
        = f.object.new_record? ?  'New Feed' : 'Edit Feed'
      %label{:for => "feed_is_default"} is_default?
      = check_box_tag "feed[is_default]",  true, @feed.is_default
      %label Title
      = f.text_field :title
      %label Url
      = f.text_field :url
      %label Author
      = f.text_field :author
      %label City
      = f.text_field :city
      %label State
      = f.text_field :state
      -#%label zipcode
      -#= f.text_field :zipcodes, value: !@feed.new_record? ? @feed.zipcodes.join(', '): ""
      %label Frequency
      = f.text_field :frequency
      %label Note's
      = f.text_field :notes




      .clear
    = submit_tag "Save"
